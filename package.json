{
  "name": "@datalayer/jupyter-ai-agents",
  "version": "0.20.0",
  "description": "Jupyter AI Agents.",
  "keywords": [
    "ai",
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "mcp",
    "agents",
    "ai-agents"
  ],
  "homepage": "https://github.com/datalayer/jupyter-ai-agents",
  "bugs": {
    "url": "https://github.com/datalayer/jupyter-ai-agents/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Datalayer",
    "email": "info@datalayer.ai"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "src/**/*.{ts,tsx}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/datalayer/jupyter-ai-agents.git"
  },
  "scripts": {
    "build": "jlpm build:lib && jlpm build:labextension:dev",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc --sourceMap",
    "build:lib:prod": "tsc",
    "build:prod": "jlpm clean && jlpm build:lib:prod && jlpm build:labextension",
    "clean": "jlpm clean:lib",
    "clean:all": "jlpm clean:lib && jlpm clean:labextension && jlpm clean:lintcache",
    "clean:labextension": "rimraf jupyter_ai_agents/labextension jupyter_ai_agents/_version.py",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:lintcache": "rimraf .eslintcache .stylelintcache",
    "eslint": "jlpm eslint:check --fix",
    "eslint:check": "eslint . --cache --ext .ts,.tsx",
    "example": "run-p jupyter:start example:chat",
    "example:chat": "EXAMPLE=Example webpack serve --mode development",
    "install:extension": "jlpm build",
    "jupyter:start": "./dev/sh/start-jupyter-server.sh",
    "lint": "jlpm stylelint && jlpm prettier && jlpm eslint",
    "lint:check": "jlpm stylelint:check && jlpm prettier:check && jlpm eslint:check",
    "prettier": "jlpm prettier:base --write --list-different",
    "prettier:base": "prettier \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "prettier:check": "jlpm prettier:base --check",
    "stylelint": "jlpm stylelint:check --fix",
    "stylelint:check": "stylelint --cache \"style/**/*.css\"",
    "test": "jest --coverage",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w --sourceMap"
  },
  "dependencies": {
    "@datalayer/agent-runtimes": "^0.0.4",
    "@datalayer/jupyter-react": "^2.0.0",
    "@jupyter/collaboration": "^4.0.1",
    "@jupyter/collaboration-extension": "^4.0.1",
    "@jupyter/collaborative-drive": "^4.0.1",
    "@jupyter/docprovider": "^4.0.1",
    "@jupyter/docprovider-extension": "^4.0.1",
    "@jupyter/ydoc": "^3.0.3",
    "@jupyterlab/application": "^4.0.0",
    "@jupyterlab/apputils": "^4.0.0",
    "@jupyterlab/coreutils": "^6.0.0",
    "@jupyterlab/services": "^7.0.0",
    "@jupyterlab/settingregistry": "^4.0.0",
    "@jupyterlab/ui-components": "^4.0.0",
    "@tanstack/react-query": "^5.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "^6.22.3",
    "zod": "^4.1.13",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@jupyterlab/builder": "^4.0.0",
    "@jupyterlab/testutils": "^4.0.0",
    "@svgr/webpack": "^5.5.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/jest": "^29.2.0",
    "@types/json-schema": "^7.0.11",
    "@types/react": "18.3.20",
    "@types/react-addons-linked-state-mixin": "^0.14.22",
    "@types/react-dom": "18.3.6",
    "@types/webpack-env": "^1.18.2",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^9.1.2",
    "browserify-zlib": "^0.2.0",
    "crypto-browserify": "^3.12.1",
    "css-loader": "^6.11.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fkill-cli": "^6.0.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.2.0",
    "mini-css-extract-plugin": "^2.9.4",
    "mkdirp": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-loader": "^8.2.0",
    "prettier": "^3.0.0",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "raw-loader": "^4.0.2",
    "rimraf": "^5.0.1",
    "source-map-loader": "^1.0.2",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.4",
    "stylelint": "^16.26.1",
    "stylelint-config-recommended": "^17.0.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-prettier": "^4.0.0",
    "to-string-loader": "^1.2.0",
    "typescript": "~5.8.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "ws": "^8.18.3"
  },
  "resolutions": {
    "@atlassiansox/analytics-web-client": "npm:empty-npm-package@1.0.0",
    "@types/react": "18.3.20",
    "@types/react-dom": "18.3.6",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "overrides": {
    "@atlassiansox/analytics-web-client": "npm:empty-npm-package@1.0.0",
    "@types/react": "18.3.20",
    "@types/react-dom": "18.3.6",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "jupyterlab": {
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "jupyter_ai_agents"
        }
      }
    },
    "extension": true,
    "outputDir": "jupyter_ai_agents/labextension",
    "schemaDir": "schema",
    "webpackConfig": "./webpack.lab-config.js"
  },
  "eslintIgnore": [
    "node_modules",
    "dist",
    "coverage",
    "**/*.d.ts",
    "tests",
    "**/__tests__",
    "ui-tests"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "overrides": [
      {
        "files": "package.json",
        "options": {
          "tabWidth": 4
        }
      }
    ]
  },
  "stylelint": {
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-standard",
      "stylelint-prettier/recommended"
    ],
    "rules": {
      "csstree/validator": true,
      "property-no-vendor-prefix": null,
      "selector-class-pattern": "^([a-z][A-z\\d]*)(-[A-z\\d]+)*$",
      "selector-no-vendor-prefix": null,
      "value-no-vendor-prefix": null
    }
  }
}
